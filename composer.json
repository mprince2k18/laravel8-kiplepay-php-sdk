{
    "name": "mprince/kiplepay",
    "description": "The SDK mainly shows how to access the kiplepay gateway.",
    "type": "library",
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "guzzlehttp/guzzle": "^7.3.0",
        "monolog/monolog": "^2.3.5",
        "symfony/http-foundation": "^5.3.7 ",
        "symfony/event-dispatcher": "5.3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evans",
            "email": "evans.yang@greenpacket.com.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "Greenpacket\\KiplePay\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greenpacket\\KiplePay\\Tests\\": "tests"
        }
    }
}
